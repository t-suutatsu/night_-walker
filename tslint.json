{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "no-angle-bracket-type-assertion": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "src/types/*.d.ts"
    ]
  }
}